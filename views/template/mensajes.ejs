    <% if (message.length >0) { %>
        <div class="row">
            <div class="container p-1">
              <div class="alert alert-success alert-dismissible fade show" role="alert">
                    <%= message %> 
                <button class="close" data-dismiss="alert" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
            </div>
          </div>
        </div>
    <% } %>   
    
